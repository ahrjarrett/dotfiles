[user]
	email = ahrjarrett@gmail.com
	name = Andrew Jarrett
[core]
	excludesfile = $HOME/.gitignore_global
[alias]
	co = checkout
	cherry = cherry-pick
	graph = log --graph --oneline
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)(%an)%Creset' --abbrev-commit
[color]
	ui = true

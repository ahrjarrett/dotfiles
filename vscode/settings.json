{
  // ELM
  "elm.compiler": "./node_modules/.bin/elm",
  "elm.makeCommand": "./node_modules/.bin/elm-make",

  // ORG MODE:
  "org.todoKeywords": ["TODO", "DONE", "REFACTOR", "ADD-TEST"],
  "editor.hover.enabled": false,
  "workbench.list.keyboardNavigation": "simple",
  "editor.maxTokenizationLineLength": 10000,
  "editor.fontFamily": "Operator Mono",
  "editor.fontSize": 15,
  "editor.fontWeight": "400",
  //   "editor.fontWeight": "300",

  "editor.fontLigatures": true,
  "editor.detectIndentation": false,

  "editor.tabCompletion": "on",
  "editor.tabSize": 8,
  "editor.renderLineHighlight": "line",
  "editor.showFoldingControls": "always",
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/node_modules": true
  },
  // EXTENSIONS:
  "bracketPairColorizer.forceIterationColorCycle": false,
  "vim.handleKeys": { "<C-c>": false, "<C-g>": false },
  "vim.useSystemClipboard": true,
  "vim.overrideCopy": true,
  "vim.highlightedyank.enable": true,
  "vim.debug.silent": true,
  // TESTING
  "vim.startInInsertMode": false,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.easymotion": true,
  "vim.easymotionDimBackground": true,
  "vim.easymotionJumpToAnywhereRegex": "\\b[A-Za-z0-9]|[A-Za-z0-9]\\b|_.|#.|[a-z][A-Z]",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.incsearch": true,
  "vim.hlsearch": true,
  // CONSIDER LATER:
  "vim.vimrc.enable": false,
  "vim.startofline": true,
  "vim.foldfix": true,
  "vim.mouseSelectionGoesIntoVisualMode": true,
  "vim.scroll": 16,
  "vim.replaceWithRegister": false,
  "editor.autoIndent": "full",
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "line",
  "editor.letterSpacing": 0.3,
  "editor.lineHeight": 18,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "off",
  "search.showLineNumbers": true,
  "zenMode.centerLayout": false,
  "vim.leader": "<space>",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#EEAD0B",
    "editorCursor.background": "#efeae9",
    "tab.activeModifiedBorder": "#ff0000",
    "tab.inactiveModifiedBorder": "#ff0000",
    "tab.unfocusedActiveModifiedBorder": "#ff0000",
    "tab.unfocusedInactiveModifiedBorder": "#ff0000",
    "[Night Owl]": {
      //"activityBar.background": "#000C1D",
      "activityBar.background": "#000C1D",
      "activityBar.border": "#102A44",
      "sideBar.background": "#001122",
      "sideBar.border": "#102A44",
      "sideBar.foreground": "#8BADC1"
    },
    "[Night Owl (No Italics)]": {
      "activityBar.background": "#000C1D",
      "activityBar.border": "#102A44",
      "sideBar.background": "#001122",
      "sideBar.border": "#102A44",
      "sideBar.foreground": "#8BADC1"
    },
    "[leuven-light]": {}
  },
  "breadcrumbs.enabled": true,
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "metals.javaHome": "/Users/andrewjarrett/.jabba/jdk/adopt@1.11.0-7/Contents/Home",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorTheme": "Better Solarized Light",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.multiCursorModifier": "alt",
  "editor.accessibilitySupport": "on",
  "editor.find.cursorMoveOnType": false,
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.external.osxExec": "iTerm.app",
  "git.allowNoVerifyCommit": true,
  "editor.snippetSuggestions": "top",
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "workbench.iconTheme": "ayu",
  "fp-ts-codegen.foldName": "match",
  "fp-ts-codegen.tagName": "_type",
  "fp-ts-codegen.encoding": "fp-ts",
  "fp-ts-codegen.handlersStyle": "record",
  "fp-ts-codegen.matcheeName": "fa",
  "fp-ts-codegen.handlersName": "handlers",
  "yassnippet.mapping": {
    "shellscript": "sh-mode",
    "typescript": "typescript-mode"
  },
  "window.autoDetectColorScheme": true,
  "terminal.integrated.tabs.location": "left",

  //// REMOVING TO CUSTOMIZE VSpaceCode (which-key like thing)
  // "vim.normalModeKeyBindingsNonRecursive": [
  //   {
  //     "before": ["<space>", "f", "f"],
  //     "after": [],
  //     "commands": [{ "command": "workbench.action.quickOpen", "args": [] }]
  //   },

  //   {
  //     "before": ["<space>", "f", "s"],
  //     "after": [],
  //     "commands": [{ "command": "workbench.action.saveFile", "args": [] }]
  //   }
  // ],
  // "vim.insertModeKeyBindingsNonRecursive": [
  //   {
  //     "before": ["ctrl+o"],
  //     "after": ["<esc>", "O"]
  //   }
  // ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<space>"],
      "commands": ["vspacecode.space"]
    },
    {
      "before": [","],
      "commands": [
        "vspacecode.space",
        {
          "command": "whichkey.triggerKey",
          "args": "m"
        }
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<space>"],
      "commands": ["vspacecode.space"]
    },
    {
      "before": [","],
      "commands": [
        "vspacecode.space",
        {
          "command": "whichkey.triggerKey",
          "args": "m"
        }
      ]
    }
  ],
  "terminal.integrated.tabs.enabled": true,
  "workbench.editor.showTabs": false,
  "git.confirmNoVerifyCommit": true,
  "whichkey.sortOrder": "nonNumberFirst",

  ///////////// VSPACECODE ///////////////////
  ///////////// VSPACECODE ///////////////////
  ///////////// VSPACECODE ///////////////////
  "vspacecode.bindingOverrides": [
    {
      "keys": "g",
      "name": "+Magit",
      "type": "bindings",
      "bindings": [
        {
          "key": "s",
          "name": "Status",
          "type": "command",
          "command": "magit.status"
        },
        {
          "key": "d",
          "name": "Diff",
          "type": "command",
          "command": "magit.diff-file"
        }
      ]
    },

    {
      "keys": ":",
      "name": "+TypeScript",
      "type": "bindings",
      "bindings": [
        {
          "key": "p",
          "name": "Peek Definition",
          "type": "command",
          "command": "editor.action.peekDefinition"
        },
        {
          "key": "r",
          "name": "Reveal Definition Aside",
          "type": "command",
          "command": "editor.action.revealDefinitionAside"
        },
        {
          "key": "h",
          "name": "Show Definition Preview Hover",
          "type": "command",
          "command": "editor.action.showDefinitionPreviewHover"
        },
        {
          "key": "R",
          "name": "Rename",
          "type": "command",
          "command": "editor.action.rename"
        },

        {
          "key": "F",
          "name": "Find All File References",
          "type": "command",
          "command": "typescript.findAllFileReferences"
        },

        {
          "key": "v",
          "name": "Select Typescript Version",
          "type": "command",
          "command": "typescript.selectTypeScriptVersion"
        },
        {
          "key": "l",
          "name": "Open TS Server Log",
          "type": "command",
          "command": "typescript.openTsServerLog"
        },
        {
          "key": "c",
          "name": "Go to Project Config",
          "type": "command",
          "command": "typescript.goToProjectConfig"
        },
        {
          "key": "+",
          "name": "Reload Projects",
          "type": "command",
          "command": "typescript.reloadProjects"
        },
        {
          "key": "_",
          "name": "Restart TS Server",
          "type": "command",
          "command": "typescript.restartTsServer"
        }
      ]
    },

    /***
      {
    "key": "f2",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  }
  {
    "key": "ctrl+g ctrl+p",
